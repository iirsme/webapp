<div class="col-xs-4 col-md-4">
  <label><%= field.full_label %></label>
  <%= f.text_field field.name, class: "form-control form-style variable", disabled: false,
      oninput: "this.setCustomValidity('')",
      oninvalid: "this.setCustomValidity('Valor incorrecto para esta variable.')",
      :onchange => "validate_field('#{field.validation_type}', this);" %>
</div>
