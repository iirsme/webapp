$('.step3').on ('ajax:success', function (event, data) { debugger;
  $('.wizard-step').each(function(index, item) {
    $(item).removeClass('active');
  });
  $('.step3').addClass('active');

  $('.wizard-pane').each(function(index, item) {
    $(item).removeClass('active');
  });
  $('#step3').addClass('active');

  $('#research-users').html("<%= j (render 'research_users/users.html') %>");
});